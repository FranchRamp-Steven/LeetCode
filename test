import java.util.HashMap;
import java.util.Map;

class Solution {
    public boolean containsDuplicate(int[] nums) {
        Map<Integer, Integer> mp = new HashMap<Integer, Integer>();

        for(int ele : nums){
            if(mp.get(ele) != null) return true;
            mp.put(ele,1);
        }
        
        return false;
    }
}
